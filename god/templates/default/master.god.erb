#
# Generated by Chef for <%= node[:hostname] %>
#
God.load "/etc/god/conf.d/*.god"

<% @rooms.each do |room| %>
God.contact(:campfire) do |c|
    c.name = "campfire-<%= room[:id] %>"
    c.subdomain = "<%= room[:subdomain] %>"
    c.token = "<%= room[:token] %>"
    c.room = "<%= room[:id] %>"
end
<% end %>
